<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head.ejs',title="MYSHOWS" ) %>
</head>

<body>
    <%- include('../partials/nav.ejs') %>
        <div class="container">
            <% data.results.forEach(element=> {%>
                <figure>
                    <a href=<%=`/moviesDetails/${element.id}`%>><img id="img"
                            src=<%=`https://image.tmdb.org/t/p/w500${element.poster_path}`%>></a>
                    <figcaption id="captions">
                        <%= element.title %>
                            <br>
                            <%= "⭐️" %>
                                <%= element.vote_average %>
                                    <br>
                    </figcaption>

                </figure>

                <%}); %>
        </div>
        <script src="/main.js"></script>
</body>

</html>